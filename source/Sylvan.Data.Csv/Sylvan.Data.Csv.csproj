<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
    <Version>0.6.2</Version>
    <Description>A .NET library for reading and writing delimited CSV data.</Description>
    <PackageTags>csv;delimited;datareader;async</PackageTags>    
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  <PropertyGroup Condition="$(TargetFramework) == 'netstandard2.1'">
    <DedupeStrings>True</DedupeStrings>
    <DefineConstants>DEDUPE_STRINGS</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="../Sylvan.Data/SchemaTable.cs" />
    <Compile Condition="$(DedupeStrings) == 'True'" Include="../Sylvan.Common/StringPool.cs" />
    <Compile Condition="$(DedupeStrings) == 'True'" Include="../Sylvan.Common/StringPoolFast.cs" />
  </ItemGroup>
</Project>
